		}
	
	}
	
	function getDetails(xml) {
	
		var type = $("#studentTypeDDL :selected").text();
		var xmlF, spTag;
		if (type == "Domestic Student") {
			xmlF = "domestic.xml";
		} else if (type == "International Student") {
			xmlF = "international.xml";
		}
		
		var course = $("#courseTypeDDL :selected").text();
		var sp = $("#periodTypeDDL option:selected").val();
		
		$("#courseChosen").text(course);
		$("#courseDuration").text($("#periodTypeDDL option:selected").text());
		
		$.ajax({
			type: "GET",
			url: xmlF,
			dataType: "xml",
			success: function(xml) {
			
				if (sp == 2015) {
					spTag = "fee15"
				} else if (sp == 2016) {
					spTag = "fee16"
				}
				
				$(xml).find("course").each(function() {
				
					if ($(this).find("program").text() == course) {
					
						$("#subjectFee").text(formatNumber($(this).find(spTag).text()));
						$("#subjectFee").val($(this).find(spTag).text());
						$("#stri").text($(this).find("terms").text());
						$("#stri").val($(this).find("terms").text());
						$("#numSubject").text($(this).find("subjects").text());
						$("#numSubject").val($(this).find("subjects").text());
						var fee = $(this).find(spTag).text();
						var nos = $(this).find("subjects").text();
						var ntri = $(this).find("terms").text();
						var total = fee*nos;
						$("#totalFee").text(formatNumber(total.toFixed(2)));
						var ssa = nos*42.90;
						$("#SSAF").text(formatNumber(ssa.toFixed(2)));
						var sp = $("#periodTypeDDL :selected").text();
						getCourseRange(sp,ntri);
						var oshc = $("#OSHC").val();
						var nett = parseFloat(total)+parseFloat(ssa)+parseFloat(oshc);
						$("#payableFee").text(formatNumber(nett.toFixed(2)));
					
					}
				
				});
			
			} 
		});
	
	}
	
	function getDomestic2(xml) {
								
		$("#courseTypeDDL").append('<option id="postgraduate" disabled>- Postgraduate -</option>');
		$("#courseTypeDDL").append('<option id="jmaster" disabled>- Joint Master -</option>');
	
		$(xml).find("course").each(function() {
			
			if ($(this).find("type").text() == "Postgraduate") {
			
				$("#courseTypeDDL option#postgraduate").after('<option>'+$(this).find("program").text()+'</option>');
			
			} else if ($(this).find("type").text() == "Joint Master") {
			
				$("#courseTypeDDL option#jmaster").after('<option>'+$(this).find("program").text()+'</option>');
			
			}
		
		});
			
		$("#courseTypeDDL").attr('selectedIndex', 1);
		$("#typeStudent").text("Domestic Student");
		getDetails();
	
	}
	
	function getInternational2(xml) {
	
		$("#courseTypeDDL").append('<option id="diploma" disabled>- Diploma -</option>');
		$("#courseTypeDDL").append('<option id="undergraduate" disabled>- Undergraduate -</option>');
		$("#courseTypeDDL").append('<option id="postgraduate" disabled>- Postgraduate -</option>');
		$("#courseTypeDDL").append('<option id="jmaster" disabled>- Joint Master -</option>');
	
		$(xml).find("course").each(function() {
			
			if ($(this).find("type").text() == "Postgraduate") {
			
				$("#courseTypeDDL option#postgraduate").after('<option>'+$(this).find("program").text()+'</option>');
			
			} else if ($(this).find("type").text() == "Joint Master") {
			
				$("#courseTypeDDL option#jmaster").after('<option>'+$(this).find("program").text()+'</option>');
			
			} else if ($(this).find("type").text() == "Diploma") {
			
				$("#courseTypeDDL option#diploma").after('<option>'+$(this).find("program").text()+'</option>');
			
			} else if ($(this).find("type").text() == "Undergraduate") {
			
				$("#courseTypeDDL option#undergraduate").after('<option>'+$(this).find("program").text()+'</option>');
			
			}
		
		});
			
		$("#courseTypeDDL").attr('selectedIndex', 1);
		$("#typeStudent").text("International Student");
		getDetails();
	
	}
	
	
</script>
