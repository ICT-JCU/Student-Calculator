<script type="text/javascript">
	$(document).ready(function() {
	
		$.ajax({
			type: "GET",
			url: "domestic.xml",
			dataType: "xml",
			success: getDomestic 
		});
		
		//Type Student Selected
		$("#typeDDL").change(function() {
		
			var value = $("select#typeDDL option:selected").text();
			if (value == "Domestic Student") {
			
				$("#courseDDL").empty();
				$.ajax({
					type: "GET",
					url: "domestic.xml",
					dataType: "xml",
					success: getDomestic 
				});
			
			}
			if (value == "International Student") {
			
				$("#courseDDL").empty();
				$.ajax({
					type: "GET",
					url: "international.xml",
					dataType: "xml",
					success: getInternational
				});
			
			}
			$("#spDDL").attr('selectedIndex', 0);
		
		});
		
		//Course Selected
		$("#courseDDL").change(function() {
		
			getFPS();
		
		});
		
		//Study Period Selected
		$("#spDDL").change(function() {
			
			getFPS();
			getDate();
		
		});
		
		//Number of Subject Selected
		$("#nosDDL").change(function() {
		
			getFTBP();
		
		});
		
		//Amount Saved Entered
		$("#ASaved").keyup(function() {
		
			getTSR();
		
		});
		
		//Saving Type Selected
		$("select#pType").change(function() {
		
			getSRP();
		
		});
		
	/*	//Due Date Changed
		$("#duedate").change(function() {
		
			getSRP();
		
		});*/
		
		//Load Date
		getDate();
		
	});
	
	function getSRP() {
	
	//	var curdate = $("#curdate").val();
		var duedate = $("#dueDate").val();
		var stype = $("select#pType").val();
		var nod = $("#durationPeriod").val();
	//	var duration = new Date(Date.parse(duedate)-Date.parse(curdate));
	/*	if (duration < 0) {
			alert("Time Duration is invalid");
		}
		if (stype == "week") {
			var nod = duration/1000/60/60/24/7;
			$("span#pType").text("Week");
			var period = Math.floor(nod);
			var asave = $("#TFP").text()-$("#ASaved").val();
			
			if (nod < 1) {
				alert("You have less than a week to do the saving!");
				$("#SRP").text(asave.toFixed(2));
			} else {
				$("#SRP").text((asave/period).toFixed(2));
			}
			
			$("#nop").text(period);
			$("#sType").text($("span#pType").text()+"(s)");
		} else if (stype == "fornight") {
			var nod = duration/1000/60/60/24/7/2;
			$("span#pType").text("Fortnight");
			var period = Math.floor(nod);
			
		var asave = $("#TFP").text()-$("#ASaved").val();
		$("#SRP").text((asave/period).toFixed(2));
		$("#nop").text(period);
		$("#sType").text($("span#pType").text()+"(s)");
		} else if (stype == "month") {
			var nod = duration/1000/60/60/24/30;
			$("span#pType").text("Month");
			var period = Math.floor(nod);
			
		var asave = $("#TFP").text()-$("#ASaved").val();
		$("#SRP").text((asave/period).toFixed(2));
		$("#nop").text(period);
		$("#sType").text($("span#pType").text()+"(s)");
		}*/
		if (nod < 7) {
		//	var duration = nod/7;
			$("span#pType").text("Week");
		//	var period = Math.floor(duration);
		//	var asave = $("#TFP").text()-$("#ASaved").val();
			var tsr = $("#TSR").val();
			alert("You have less than a week to do the saving!");
			$("#SRP").text(formatNumber(tsr));
			
			$("#nop").text(nod+" Day(s)");
		//	$("#sType").text($("span#pType").text()+"(s)");
		} else if (stype == "week" & nod >= 7) {
			var duration = nod/7;
			$("span#pType").text("Week");
			var period = Math.floor(duration);
		//	var asave = $("#TFP").text()-$("#ASaved").val();
			var tsr = $("#TSR").val();
			
			if (period < 1) {
				alert("You have less than a week to do the saving!");
				$("#SRP").text(formatNumber(tsr.toFixed(2)));
			} else {
				$("#SRP").text(formatNumber((tsr/period).toFixed(2)));
			}
			
			var leftover = nod-(period*7);
			
			if (leftover == 0) {
				$("#nop").text(period+" Week(s)");
			} else {
				$("#nop").text(period+" Week(s) and "+leftover+" Day(s)");
			}
		//	$("#sType").text($("span#pType").text()+"(s)");
		} else if (stype == "fortnight") {
			var duration = nod/14;
			$("span#pType").text("Fortnight");
			var period = Math.floor(duration);
		//	var asave = $("#TFP").text()-$("#ASaved").val();
			var tsr = $("#TSR").val();
			
			if (period < 1) {
				alert("You have less than a fortnight to do the saving!");
				$("#SRP").text(formatNumber(tsr));
			} else {
				$("#SRP").text(formatNumber((tsr/period).toFixed(2)));
			}
			
			var leftover = nod-(period*14);
			
			if (leftover == 0) {
				$("#nop").text(period+" Fortnight(s)");
			} else {
				$("#nop").text(period+" Fortnight(s) and "+leftover+" Day(s)");
			}
		//	$("#sType").text($("span#pType").text()+"(s)");
		} else if (stype == "month") {
			var duration = nod/31;
			$("span#pType").text("Month");
			var period = Math.floor(duration);
		//	var asave = $("#TFP").text()-$("#ASaved").val();
			var tsr = $("#TSR").val();
			
			if (period < 1) {
				alert("You have less than a month to do the saving!");
				$("#SRP").text(formatNumber(tsr));
			} else {
				$("#SRP").text(formatNumber((tsr/period).toFixed(2)));
			}
			
			var leftover = nod-(period*31);
			
			if (leftover == 0) {
				$("#nop").text(period+" Month(s)");
			} else {
				$("#nop").text(period+" Month(s) and "+leftover+" Days");
			}
		//	$("#sType").text($("span#pType").text()+"(s)");
		}
	
	}
	
	function getTSR() {
	
		var amount = $("#ASaved").val();
		if (isNaN(amount)) {
			alert("Please enter Numberic value only");
			$("#ASaved").val("");
			amount = 0;
		}
		var tfp = $("#TFP").val();
		var asave = tfp-amount;
		$("#TSR").text(formatNumber(asave.toFixed(2)));
		$("#TSR").val(asave.toFixed(2));
		if (asave <= 0) {
			alert("You have enough saving for the payment");
		}
		getSRP();
	
	}
	
	function getDate() {
		
		var d = new Date();
		var yyyy = d.getFullYear();
		var mm = d.getMonth()+1;
		if (mm < 10) { mm = "0"+mm; }
		var dd = d.getDate();
		if (dd < 10) { dd = "0"+dd; }
		var todate = yyyy + "-" + mm + "-" + dd;
	//	alert(todate);
	//	$("#curdate").val(todate);
	
		var sp = $("#spDDL :selected").text();
		if (sp == "SP22 July 2015") {
			$("#startDate").text("20 July 2015");
			$("#dueDate").text("03 August 2015");
			$("#endDate").text("23 October 2015");
			$("#dueDate").val("2015/08/03");
		} else if (sp == "SP23 November 2015") {
			$("#startDate").text("16 November 2015");
			$("#dueDate").text("01 December 2015");
			$("#endDate").text("26 February 2016");
			$("#dueDate").val("2015-12-01");	
		} else if (sp == "SP21 March 2016") {
			$("#startDate").text("21 March 2016");
			$("#dueDate").text("04 April 2016");
			$("#endDate").text("24 June 2016");
			$("#dueDate").val("2016-04-04");	
		} else if (sp == "SP22 July 2016") {
			$("#startDate").text("18 July 2016");
			$("#dueDate").text("02 August 2016");
			$("#endDate").text("14 October 2016");
			$("#dueDate").val("2016-08-02");	
		} else if (sp == "SP23 November 2016") {
			$("#startDate").text("21 November 2016");
			$("#dueDate").text("05 December 2016");
			$("#endDate").text("03 March 2016");
			$("#dueDate").val("2016-12-05");	
		}
		
		var duedate = $("#dueDate").val();
	//	duedate = "2015/06/07";
		var duration = new Date(Date.parse(duedate)-Date.parse(todate));
		var nod = Math.floor(duration/1000/60/60/24);
		$("#durationPeriod").text(nod+" days");
		$("#durationPeriod").val(nod);
	//	alert($("#durationPeriod").val());
	
	}
	
	function getFTBP() {
	
		var nos = $("#nosDDL :selected").text();
		var fps = $("#FPS").val();
		var ftbp = nos*fps;
		$("#FTBP").text(formatNumber(ftbp.toFixed(2)));
		$("#FTBP").val(ftbp.toFixed(2));
		var ssa = nos*42.90;
		$("#SSA").text(formatNumber(ssa.toFixed(2)));
		$("#SSA").val(ssa.toFixed(2));
		var tfp = ftbp+ssa;
		$("#TFP").text(formatNumber(tfp.toFixed(2)));
		$("#TFP").val(tfp.toFixed(2));
		getTSR();
	
	}
	
	function getFPS(xml) {
	
		var type = $("#typeDDL :selected").text();
		var xmlF, spTag;
		if (type == "Domestic Student") {
			xmlF = "domestic.xml";
		} else if (type == "International Student") {
			xmlF = "international.xml";
		}
		
		var course = $("#courseDDL :selected").text();
		var sp = $("#spDDL option:selected").val();
		
		$("#selCourse").text(course);
		$("#studyPeriod").text($("#spDDL option:selected").text());
		
		$.ajax({
			type: "GET",
			url: xmlF,
			dataType: "xml",
			success: function(xml) {
			
				if (sp == 2015) {
					spTag = "fee15"
				} else if (sp == 2016) {
					spTag = "fee16"
				}
				
				$(xml).find("course").each(function() {
				
					if ($(this).find("program").text() == course) {
					
						$("#FPS").text(formatNumber($(this).find(spTag).text()));
						$("#FPS").val($(this).find(spTag).text());
						getFTBP();
					
					}
				
				});
			
			} 
		});
	
	}
	
	function getDomestic(xml) {
								
		$("#courseDDL").append('<option id="postgraduate" disabled>- Postgraduate -</option>');
		$("#courseDDL").append('<option id="jmaster" disabled>- Joint Master -</option>');
	
		$(xml).find("course").each(function() {
			
			if ($(this).find("type").text() == "Postgraduate") {
			
				$("#courseDDL option#postgraduate").after('<option>'+$(this).find("program").text()+'</option>');
			
			} else if ($(this).find("type").text() == "Joint Master") {
			
				$("#courseDDL option#jmaster").after('<option>'+$(this).find("program").text()+'</option>');
			
			}
		
		});
			
		$("#courseDDL").attr('selectedIndex', 1);
		$("#studentType").text("Domestic Student");
		getFPS();
	
	}
	
	function getInternational(xml) {
	
		$("#courseDDL").append('<option id="diploma" disabled>- Diploma -</option>');
		$("#courseDDL").append('<option id="undergraduate" disabled>- Undergraduate -</option>');
		$("#courseDDL").append('<option id="postgraduate" disabled>- Postgraduate -</option>');
		$("#courseDDL").append('<option id="jmaster" disabled>- Joint Master -</option>');
	
		$(xml).find("course").each(function() {
			
			if ($(this).find("type").text() == "Postgraduate") {
			
				$("#courseDDL option#postgraduate").after('<option>'+$(this).find("program").text()+'</option>');
			
			} else if ($(this).find("type").text() == "Joint Master") {
			
				$("#courseDDL option#jmaster").after('<option>'+$(this).find("program").text()+'</option>');
			
			} else if ($(this).find("type").text() == "Diploma") {
			
				$("#courseDDL option#diploma").after('<option>'+$(this).find("program").text()+'</option>');
			
			} else if ($(this).find("type").text() == "Undergraduate") {
			
				$("#courseDDL option#undergraduate").after('<option>'+$(this).find("program").text()+'</option>');
			
			}
		
		});
			
		$("#courseDDL").attr('selectedIndex', 1);
		$("#studentType").text("International Student");
		getFPS();
	
	}
	
	function formatNumber(num) {
		return num.toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, "$1,")
	}
	
	
</script>
<!--JavaScript for Income Expense Calculator-->
<script type="text/javascript">
	$(document).ready(function() {
	
		$("#txtWage").keyup(function() {
			getBalance();
		});
		$("#txtSupport").keyup(function() {
			getBalance();
		});
		$("#txtScho").keyup(function() {
			getBalance();
		});
		$("#txtIOther").keyup(function() {
			getBalance();
		});
		$("#txtAcc").keyup(function() {
			getBalance();
		});
		$("#txtGro").keyup(function() {
			getBalance();
		});
		$("#txtTran").keyup(function() {
			getBalance();
		});
		$("#txtEOther").keyup(function() {
			getBalance();
		});
		$("#WageMode").change(function() {
			getBalance();
		});
		$("#SupportMode").change(function() {
			getBalance();
		});
		$("#SchoMode").change(function() {
			getBalance();
		});
		$("#IOtherMode").change(function() {
			getBalance();
		});
		$("#AccMode").change(function() {
			getBalance();
		});
		$("#GroMode").change(function() {
			getBalance();
		});
		$("#TranMode").change(function() {
			getBalance();
		});
		$("#EOtherMode").change(function() {
			getBalance();
		});
	
	});
	
	function getBalance() {
	
		var wage = +$("#txtWage").val();
		var support = +$("#txtSupport").val();
		var scho = +$("#txtScho").val();
		var iother = +$("#txtIOther").val();
		var acc = +$("#txtAcc").val();
		var gro = +$("#txtGro").val();
		var tran = +$("#txtTran").val();
		var eother = +$("#txtEOther").val();
		var wageMode = $("#WageMode").val();
		var supportMode = $("#SupportMode").val();
		var schoMode = $("#SchoMode").val();
		var iotherMode = $("#IOtherMode").val();
		var accMode = $("#AccMode").val();
		var groMode = $("#GroMode").val();
		var tranMode = $("#TranMode").val();
		var eotherMode = $("#EOtherMode").val();
		
		if (isNaN(wage)) {
			alert("Please enter Numberic value only");
			$("#txtWage").val("");
			wage = 0;
		}
		if (isNaN(support)) {
			alert("Please enter Numberic value only");
			$("#txtSupport").val("");
			support = 0;
		}
		if (isNaN(scho)) {
			alert("Please enter Numberic value only");
			$("#txtScho").val("");
			scho = 0;
		}
		if (isNaN(iother)) {
			alert("Please enter Numberic value only");
			$("#txtIOther").val("");
			iother = 0;
		}
		if (isNaN(acc)) {
			alert("Please enter Numberic value only");
			$("#txtAcc").val("");
			acc = 0;
		}
		if (isNaN(gro)) {
			alert("Please enter Numberic value only");
			$("#txtGro").val("");
			gro = 0;
		}
		if (isNaN(tran)) {
			alert("Please enter Numberic value only");
			$("#txtTran").val("");
			tran = 0;
		}
		if (isNaN(eother)) {
			alert("Please enter Numberic value only");
			$("#txtEOther").val("");
			eother = 0;
		}
		
		//Income Section
		var witotal = getWeekValue(wage,wageMode)+getWeekValue(support,supportMode)+getWeekValue(scho,schoMode)+getWeekValue(iother,iotherMode);
		var fitotal = getFortValue(wage,wageMode)+getFortValue(support,supportMode)+getFortValue(scho,schoMode)+getFortValue(iother,iotherMode);
		var mitotal = getMonthValue(wage,wageMode)+getMonthValue(support,supportMode)+getMonthValue(scho,schoMode)+getMonthValue(iother,iotherMode);
	
		if (isNaN(witotal) || isNaN(fitotal) || isNaN(mitotal) || witotal == "" || fitotal == "" || mitotal == "") {
			$("#iweek").text("0.00");
			$("#ifort").text("0.00");
			$("#imonth").text("0.00");
		} else {
			$("#iweek").text(formatNumber((parseFloat(witotal)).toFixed(2)));
			$("#ifort").text(formatNumber((parseFloat(fitotal)).toFixed(2)));
			$("#imonth").text(formatNumber((parseFloat(mitotal)).toFixed(2)));
		}
		
		//Expense Section
		var wetotal = getWeekValue(acc,accMode)+getWeekValue(gro,groMode)+getWeekValue(tran,tranMode)+getWeekValue(eother,eotherMode);
		var fetotal = getFortValue(acc,accMode)+getFortValue(gro,groMode)+getFortValue(tran,tranMode)+getFortValue(eother,eotherMode);
		var metotal = getMonthValue(acc,accMode)+getMonthValue(gro,groMode)+getMonthValue(tran,tranMode)+getMonthValue(eother,eotherMode);
	
		if (isNaN(wetotal) || isNaN(fetotal) || isNaN(metotal) || wetotal == "" || fetotal == "" || metotal == "") {
			$("#eweek").text("0.00");
			$("#efort").text("0.00");
			$("#emonth").text("0.00");
		} else {
			$("#eweek").text(formatNumber((parseFloat(wetotal)).toFixed(2)));
			$("#efort").text(formatNumber((parseFloat(fetotal)).toFixed(2)));
			$("#emonth").text(formatNumber((parseFloat(metotal)).toFixed(2)));
		}
		
		//Balance Section
		var wbalance = witotal-wetotal;
		var fbalance = fitotal-fetotal;
		var mbalance = mitotal-metotal;
		$("#bweek").text(formatNumber((parseFloat(wbalance)).toFixed(2)));
		$("#bfort").text(formatNumber((parseFloat(fbalance)).toFixed(2)));
		$("#bmonth").text(formatNumber((parseFloat(mbalance)).toFixed(2)));
		
		if (wbalance < 0 || fbalance < 0 || mbalance < 0) {
			$("#status").text("Your Financial Status is Poor and at Risk!");
		} else if (wage == "" &  support == "" & scho == "" &iother == "" & acc == "" & gro == "" & tran == "" & eother == "") {
			$("#status").text("");
		} else {
			$("#status").text("Your Financial Status is Good Enough");
		}
	
	}
	
	function getWeekValue(value, mode) {
	
		if (mode == "week") {
			return value;
		} else if (mode == "fortnight") {
			return value/2;
		} else if (mode == "month") {
			return (value/30)*7;
		}
	
	}
	
	function getFortValue(value, mode) {
	
		if (mode == "week") {
			return value*2;
		} else if (mode == "fortnight") {
			return value;
		} else if (mode == "month") {
			return (value/30)*14;
		}
	
	}
	
	function getMonthValue(value, mode) {
	
		if (mode == "week") {
			return (value/7)*30;
		} else if (mode == "fortnight") {
			return (value/14)*30;
		} else if (mode == "month") {
			return  value;
		}
	
	}
	
	
</script>
</script>
<!--JavaScript for General Fee Calculator-->
<script type="text/javascript">
	$(document).ready(function() {
	
		$.ajax({
			type: "GET",
			url: "domestic.xml",
			dataType: "xml",
			success: getDomestic2 
		});
	
		//Type Student Selected
		$("#studentTypeDDL").change(function() {
		
			var value = $("select#studentTypeDDL option:selected").text();
			if (value == "Domestic Student") {
			
				$("#courseTypeDDL").empty();
				$.ajax({
					type: "GET",
					url: "domestic.xml",
					dataType: "xml",
					success: getDomestic2 
				});
			
			}
			if (value == "International Student") {
			
				$("#courseTypeDDL").empty();
				$.ajax({
					type: "GET",
					url: "international.xml",
					dataType: "xml",
					success: getInternational2
				});
			
			}
			$("#periodTypeDDL").attr('selectedIndex', 0);
		
		});
		
		//Course Selected
		$("#courseTypeDDL").change(function() {
		
			getDetails();
		
		});
		
		//Study Period Selected
		$("#periodTypeDDL").change(function() {
			
			getDetails();
		
		});
		
	
	});
	
	function getCourseRange(start, trimester) {
			
		if (start == "SP22 July 2015") {
			$("#comDate").text("20 July 2015");
			if (trimester == 3) {
				$("#expectDate").text("24 June 2016");
				var oshc = 600;
				$("#OSHC").text(formatNumber(oshc.toFixed(2)));
				$("#OSHC").val(oshc);
			} else if (trimester == 6) {
				$("#expectDate").text("23 June 2017");
				var oshc = 1169;
				$("#OSHC").text(formatNumber(oshc.toFixed(2)));
				$("#OSHC").val(oshc);
			} else if (trimester == 8) {
				$("#expectDate").text("23 February 2018");
				var oshc = 1580;
				$("#OSHC").text(formatNumber(oshc.toFixed(2)));
				$("#OSHC").val(oshc);
			}
		} else if (start == "SP23 November 2015") {
			$("#comDate").text("16 November 2015");
			if (trimester == 3) {
				$("#expectDate").text("14 October 2016");
				var oshc = 600;
				$("#OSHC").text(formatNumber(oshc.toFixed(2)));
				$("#OSHC").val(oshc);
			} else if (trimester == 6) {
				$("#expectDate").text("20 October 2017");
				var oshc = 1169;
				$("#OSHC").text(formatNumber(oshc.toFixed(2)));
				$("#OSHC").val(oshc);
			} else if (trimester == 8) {
				$("#expectDate").text("22 June 2018");
				var oshc = 1580;
				$("#OSHC").text(formatNumber(oshc.toFixed(2)));
				$("#OSHC").val(oshc);
			}
		} else if (start == "SP21 March 2016") {
			$("#comDate").text("21 March 2016");
			if (trimester == 3) {
				$("#expectDate").text("03 March 2017");
				var oshc = 600;
				$("#OSHC").text(formatNumber(oshc.toFixed(2)));
				$("#OSHC").val(oshc);
			} else if (trimester == 6) {
				$("#expectDate").text("09 March 2018");
				var oshc = 1169;
				$("#OSHC").text(formatNumber(oshc.toFixed(2)));
				$("#OSHC").val(oshc);
			} else if (trimester == 8) {
				$("#expectDate").text("19 October 2019");
				var oshc = 1580;
				$("#OSHC").text(formatNumber(oshc.toFixed(2)));
				$("#OSHC").val(oshc);
			}
		} else if (start == "SP22 July 2016") {
